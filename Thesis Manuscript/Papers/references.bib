   title = {Separating Charges at Organic Interfaces: Effects of Disorder, Hot States, and Electric Field},
   volume = {4},
   url = {https://pubs.acs.org/doi/10.1021/jz4002339},
   year = {2013},
}
@article{Nenashev2015,
   abstract = {Hopping conduction is widely considered the dominant charge transport mechanism in disordered organic semiconductors. Although theories of hopping transport have been developed in detail for applications to inorganic amorphous materials, these theories are often out of scope for the community working with organic amorphous systems. Theoretical research on charge transport in organic systems is overwhelmed by phenomenological fittings of numerical results by equations, which often make little physical sense. The aim of the current review is to bring analytical theoretical methods to the attention of the community working with disordered organic semiconductors.},
