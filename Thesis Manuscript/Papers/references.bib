   issue = {September},
   journal = {Renewable Energy Focus},
   month = {9},
   pages = {93-99},
   title = {Cost and production of solar thermal and solar photovoltaics power plants in the United States},
   volume = {26},
   url = {https://linkinghub.elsevier.com/retrieve/pii/S1755008418300334},
   year = {2018},
}
