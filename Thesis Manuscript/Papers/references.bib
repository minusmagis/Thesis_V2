   year = {2018},
}
@article{Yang2008,
   abstract = {The charge-transfer exciton absorption and photocurrent response were studied in soln.-processed bulk heterojunction based on poly(3-hexylthiophene) donor and (6,6)-phenyl-C61-butyric acid Me ester acceptor in the near-IR wavelength region. While the exciton absorption exists only for wavelength <650 nm, direct generation of charge-transfer exciton formed between the donor and acceptor extends the absorption wavelength to 950 nm. For films with micro m thickness, the photon-to-electron conversion efficiency is .apprx.60% at l = 750 nm under reverse voltage bias and the photocurrent to dark current ratio is .apprx.8.6 at 900 nm and remains 3.6 even at 1000 nm. Photodetector with high sensitivity covering exclusively the 650-1000 nm near-IR region can be made without a low bandgap material. The charge-transfer exciton absorption coeff. and photocurrent sensitivity depend on the annealing condition which controls the donor-acceptor morphol. (c) 2008 American Institute of Physics. [on SciFinder (R)]},
