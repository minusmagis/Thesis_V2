   month = {9},
   note = {P3HT Holy Grail},
   pages = {12-28},
   title = {A review of recent plasmonic nanoparticles incorporated P3HT: PCBM organic thin film solar cells},
   volume = {36},
   url = {https://linkinghub.elsevier.com/retrieve/pii/S1566119916302257},
   year = {2016},
}
