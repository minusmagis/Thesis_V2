   keywords = {P3HT/PC71BM,cosolvent,polymer photodetectors (PPDs),sequentially solution-processed,three-phase morphology},
   month = {1},
   pages = {3856-3864},
   pmid = {29338189},
   title = {Three-Phase Morphology Evolution in Sequentially Solution-Processed Polymer Photodetector: Toward Low Dark Current and High Photodetectivity},
   volume = {10},
   url = {https://pubs.acs.org/doi/10.1021/acsami.7b15730},
