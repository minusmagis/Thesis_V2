   title = {Recombination in Polymer:Fullerene Solar Cells with Open-Circuit Voltages Approaching and Exceeding 1.0 V},
   volume = {3},
   url = {https://onlinelibrary.wiley.com/doi/10.1002/aenm.201200474},
   year = {2013},
}
