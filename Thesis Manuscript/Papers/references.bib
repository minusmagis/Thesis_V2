   title = {Factors Limiting Device Efficiency in Organic Photovoltaics},
   volume = {25},
   url = {https://onlinelibrary.wiley.com/doi/10.1002/adma.201202873},
   year = {2013},
}
@article{Jenekhe1983,
   abstract = {The rheological properties of five types of concentrated polyam-ic acid and polyimide solutions are characterized by non-Newtonian shear viscosity ~ (^ j) and primary normal stress coefficient measurements over a wide range of shear rates. Onset of non-Newtonian flow of the polyamic acid solutions was observed in the shear rate range 30 to 400s-' and of the fully im-idized polyimide solution at below 3 x 10-2s-'. Significant vis-coelastic properties exemplified by normal stresses were observed in all the solutions. The solution rheology results are discussed in the context of spin coating for the deposition of thin films. The relative magnitude of effects of non-Newtonian flow on the dynamics of spin coating is assessed with a Deborah number characteristic of the flow.},
