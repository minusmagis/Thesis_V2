   issn = {01968904},
   journal = {Energy Conversion and Management},
   keywords = {Full spectrum utilization,PV/T,Radiative transfer,Semi-transparent solar cell,Solar energy,Spectral beam splitting},
   month = {1},
   pages = {115049},
   publisher = {Elsevier Ltd},
   title = {A novel spectral beam splitting photovoltaic/thermal hybrid system based on semi-transparent solar cell with serrated groove structure for co-generation of electricity and high-grade thermal energy},
   volume = {252},
   url = {https://linkinghub.elsevier.com/retrieve/pii/S0196890421012255},
