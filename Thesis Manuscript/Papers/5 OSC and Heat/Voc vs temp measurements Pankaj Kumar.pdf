<script>
    window.addEventListener('load', function () {
        SeamlessAccessService.getEntitiesWithAllInfo("10.1063/1.3129194").then(function (res) {
            try {
                if (0 != res.length) {
                var latestAccessAcceptedEntry;
                for(var i = res.length - 1; i >= 0; i--) {
                    if(res[i].entityStatusObj.code === 1003) {
                        latestAccessAcceptedEntry = res[i];
                        break;
                    }
                }
                var latestEntity = latestAccessAcceptedEntry != null ? latestAccessAcceptedEntry : res[res.length - 1];
                console.log(JSON.stringify(latestEntity));
                var message = '';
                switch (latestEntity.entityStatusObj.code) {
                    case 1001:
                        message = `Access via the <b>` + latestEntity.entityTitle + `</b> is not supported.<br/> Please <a href='/action/ssostart?redirectUri=/doi/pdf/10.1063/1.3129194'> choose one of the other </a> institutional login options`;
                        handleNoInstitutionAccess(latestEntity.entityTitle);
                        break;
                    case 1002:
                        message = `<b>` + latestEntity.entityTitle + `</b> does not have access to this publication <br/> Please <a href='/action/ssostart?redirectUri=/doi/pdf/10.1063/1.3129194'> choose one of the other </a> institutional login options `;
                        handleNoInstitutionAccess(latestEntity.entityTitle);
                        break
                    case 1003:
                        message = "<a href='/action/ssostart?redirectUri=/doi/pdf/10.1063/1.3129194'><i class=\"icon-pencil2\"></i> Choose another institution</a>";
                        handleHasInstitutionAccess(latestEntity.entitySsoUrl, latestEntity.entityTitle);
                        break;
                    case 1004:
                        message = "<b>" + latestEntity.entityTitle + "</b> May have access for this publication <br/><a href='/action/ssostart?redirectUri=/doi/pdf/10.1063/1.3129194'><i class=\"icon-pencil2\"></i>Choose another institution</a>";
                        handleHasInstitutionAccess(latestEntity.entitySsoUrl, latestEntity.entityTitle);
                        break
                }
                $('.nothing .content-message').html(message);
            }
            $('.inst-button').removeClass('hidden');
            $('.nothing').removeClass('hidden');
            $('.seamless-access-loading').addClass('hidden');
            } catch (error) {
                console.log("Error while processing institutions: " + error);
                $('.seamless-access-loading').addClass('hidden');
                $('#institutionLogin').removeClass('hide');
            }
        }, function (error) {
            console.log("Functional error: " + error);
            $('.seamless-access-loading').addClass('hidden');
            $('#institutionLogin').removeClass('hide');
        });

        function handleHasInstitutionAccess(entitySsoUrl, entityTitle) {
            $('.seamless-access-text .inst-name').text(entityTitle);
            $('.institutionLogin-button').attr('href', entitySsoUrl);
        }

        function handleNoInstitutionAccess(entityTitle) {
            $('.institutionLogin-button').addClass('no-access-state');
            $('.seamless-access-text .inst-name').text(entityTitle);
        }
    });
</script>


        
        
        
